{
	"name": "soundbase",
	"version": "1.0.0",
	"description": "Social networking app for musicians",
	"main": "server.js",
	"scripts": {
		"test": "mocha ./test/*.js --exit",
		"start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
		"start:prod": "node server.js",
		"start:dev": "concurrently  \"nodemon --ignore 'client/*'\" \"npm run client\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Skateboardb/SoundBase.git"
	},
	"author": "Branford Shaw",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Skateboardb/SoundBase/issues"
	},
	"homepage": "https://github.com/Skateboardb/SoundBase#readme",
	"dependencies": {
		"express": "^4.17.1"
	},
	"devDependencies": {
		"mocha": "^7.1.1",
		"nodemon": "^2.0.2",
		"supertest": "^4.0.2"
	}
}
